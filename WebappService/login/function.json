{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "login/{filename=index.html}"
    },
    {
      "name": "inputBlob",
      "type": "blob",
      "path": "webapp/login/{filename}",
      "connection": "StorageConnectionString",
      "direction": "in"
    },
    
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}