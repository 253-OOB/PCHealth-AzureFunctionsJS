{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "home/{filename}" 
    },
    {
      "name": "indexBlob",
      "type": "blob",
      "path": "pchealth/app/{filename}",
      "connection": "StorageConnectionString",
      "direction": "in"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
